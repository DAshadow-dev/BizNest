{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "types": ["react", "react-native", "react-native-svg"],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@redux/*": ["src/redux/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@libs/*": ["src/libs/*"],
      "@requestComponent/*": ["src/screens/request/component/*"],
      "@assets/*": ["assets/*"],
      "@type/*": ["src/types/*"]
    }
  }
}
